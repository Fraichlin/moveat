{% extends 'base2.html.twig' %}

{% block title %}Programme{% endblock %}

{% block body %}
    <h1>Programme</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ programme.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ programme.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ programme.type }}</td>
            </tr>
            <tr>
                <th>image</th>
                <td>{{ programme.imageFile }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ programme.description }}</td>
            </tr>
            <tr>
                <th>Break_fast</th>
                <td>{{ programme.breackfast }}</td>
            </tr>
            <tr>
                <th>Dinner</th>
                <td>{{ programme.dinner }}</td>
            </tr>
            <tr>
                <th>Lunch</th>
                <td>{{ programme.lunch }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('programme_index') }}">back to list</a>

    <a href="{{ path('programme_edit', {'id': programme.id}) }}">edit</a>

    {{ include('programme/_delete_form.html.twig') }}
{% endblock %}
