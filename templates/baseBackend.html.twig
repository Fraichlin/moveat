<!DOCTYPE html>
<html lang="{{ app.request.locale }}">

<head>
    <title>{% block title %}Dashio - Bootstrap Admin Template{% endblock %}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">

    {% block css %}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

        <link href="{{ asset('lib/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('lib/font-awesome/css/font-awesome.css') }}" rel="stylesheet" />
{#        <link rel="stylesheet" type="text/css" href="{{ asset('css/zabuto_calendar.css') }}">#}
{#        <link rel="stylesheet" type="text/css" href="{{ asset('lib/gritter/css/jquery.gritter.css') }}" />#}
        <link href="{{ asset('css/style.css') }}" rel="stylesheet">
        <link href="{{ asset('css/style-responsive.css') }}" rel="stylesheet">
    {% endblock %}
    <!-- Favicons -->
    <link href="{{ asset('img/favicon.png') }}" rel="icon">
    <link href="{{ asset('img/apple-touch-icon.png') }}" rel="apple-touch-icon">




</head>

<body>
<section id="container">
    <!--header start-->
    <header class="header black-bg">
        <div class="sidebar-toggle-box">
            <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
        </div>
        <!--logo start-->
        <a href="index.html" class="logo">
            <img style="width: 150px;" src="{{ asset('img/MovEat.jpg') }}" />
        </a>
        <!--logo end-->

        <div class="top-menu">
            <ul class="nav pull-right top-menu">
                <li><a class="logout" href="{{ url('app_logout') }}">Se d√©connecter</a></li>
            </ul>
        </div>
    </header>
    <!--header end-->
    <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
    <!--sidebar start-->
    <aside>
        <div id="sidebar" class="nav-collapse">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu" id="nav-accordion">
                <p class="centered"><a href="{{ url('profileAdmin') }}">
                        {% if not app.user.getPhoto %}
                            <img src="{{ asset( 'img/profilUser.png' ) }}" class="img-circle" width="80" alt="">
                        {% else %}
                            <img src="{{ asset( 'upload/images/' ~ app.user.getPhoto ) }}" class="img-circle" width="80">
                        {% endif %}</a></p>
                <h5 class="centered">{{ app.user.prenom}} {{ app.user.nom }}</h5>
                <li class="mt">
                    <a class="{% block activation1 %}{% endblock %}" href="{{ url('homeAdmin') }}">
                        <i class="fa fa-dashboard"></i>
                        <span>Tableau de bord</span>
                    </a>
                </li>
                <li class="sub-menu">
                    <a class="{% block activation2 %}{% endblock %}" href="javascript:;">
                        <i class="fa fa-user-o"></i>
                        <span>Coachs</span>
                    </a>
                    <ul class="sub">
                        <li><a href="{{ url('listCoach') }}">Liste</a></li>
                        <li><a href="{{ url('listDemandCoach') }}">Demandes d'inscriptions</a></li>
                    </ul>
                </li>
                <li class="sub-menu">
                    <a class="{% block activation3 %}{% endblock %}" href="{{ url('listMember') }}">
                        <i class="fa fa-users"></i>
                        <span>Membres</span>
                    </a>
                </li>
            </ul>
        </div>
    </aside>

</section>
<section id="main-content">
    {% block body %}
    {% endblock %}
</section>
</body>
